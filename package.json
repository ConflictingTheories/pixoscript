{
  "name": "pixoscript",
  "version": "1.0.1",
  "description": "A custom Lua-like scripting language for Pixos game engine",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "compile": "npm run clean && cross-env NODE_ENV=production babel src --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,stories.js,__snapshots__",
    "clean": "rimraf build",
    "test": "node test.js"
  },
  "keywords": [
    "lua",
    "scripting",
    "game-engine",
    "pixos",
    "calliope",
    "pixos",
    "html5",
    "game",
    "react",
    "game",
    "webgl",
    "canvas"
  ],
  "homepage": "https://pixospritz.com",
  "repository": {
    "url": "https://github.com/ConflictingTheories/pixoscript.git",
    "type": "git"
  },
  "author": "Kyle Derby MacInnis",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.0",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-react": "^7.17.12",
    "@babel/register": "^7.17.7",
    "@types/luaparse": "^0.2.13",
    "babel-plugin-macros": "^3.1.0",
    "babel-register": "^6.26.0",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "typescript": "^5.9.3",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "dependencies": {
    "luaparse": "^0.3.1",
    "printj": "^1.3.1"
  }
}
